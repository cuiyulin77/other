import scrapy


class GovernmentalAgenciesItem(scrapy.Item):
    #  province city url
    organizations_name = scrapy.Field() #机构名字
    province = scrapy.Field() #省级
    city = scrapy.Field()#地级市
    county = scrapy.Field()#县级市
    town = scrapy.Field()#街道或乡镇
    url = scrapy.Field()# 数据采集的url
    update_time = scrapy.Field()# 更新时间
# organizations_name,province,city,county,town,url,update_time




